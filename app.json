{
  "expo": {
    "name": "juxtify",
    "slug": "juxtify",
    "version": "1.0.0",

    "scheme": "juxtify",
    "platforms": ["ios", "android"],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo.png",
          "resizeMode": "cover",
          "backgroundColor": "#000000",
          "enableFullScreenImage_legacy": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Satoshi-Black.otf",
            "./assets/fonts/Satoshi-BlackItalic.otf",
            "./assets/fonts/Satoshi-Bold.otf",
            "./assets/fonts/Satoshi-BoldItalic.otf",
            "./assets/fonts/Satoshi-Italic.otf",
            "./assets/fonts/Satoshi-Light.otf",
            "./assets/fonts/Satoshi-LightItalic.otf",
            "./assets/fonts/Satoshi-Medium.otf",
            "./assets/fonts/Satoshi-MediumItalic.otf",
            "./assets/fonts/Satoshi-Regular.otf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,

      "tsconfigPaths": true
    },

    "orientation": "portrait",
    "icon": "./assets/logo.png",

    "userInterfaceStyle": "dark",

    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#000000"
      },
      "usesCleartextTraffic": true
    }
  }
}
